<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:edm="http://www.europeana.eu/schemas/edm/" xmlns:doap="http://usefulinc.com/ns/doap#" xmlns:rdfs="https://www.w3.org/TR/rdf-schema/#">
  <edm:PersistentIdentifierScheme rdf:about="http://data.europeana.eu/pid/scheme/doi">
    <dcterms:title>Digital Object Identifier (DOI)</dcterms:title>
    <rdfs:seeAlso rdf:resource="https://www.doi.org/"/>
    <doap:maintainer>DOI Foundation</doap:maintainer>
    <edm:canonicalPattern>doi:${1}/${2}</edm:canonicalPattern>
    <edm:resolvablePattern>https://doi.org/${0}</edm:resolvablePattern>
    <edm:matchingPattern>doi:(10\.[0-9][0-9\.]*)/(.*)</edm:matchingPattern>
    <edm:matchingPattern>https?://doi\.org/doi:(10\.[0-9][0-9\.]*)/(.*)</edm:matchingPattern>
    <edm:matchingPattern>https?://dx\.doi\.org/doi:(10\.[0-9][0-9\.]*)/(.*)</edm:matchingPattern>
    <edm:matchingPattern>https?://doi\.org/(10\.[0-9][0-9\.]*)/(.*)</edm:matchingPattern>
    <edm:matchingPattern>https?://dx\.doi\.org/(10\.[0-9][0-9\.]*)/(.*)</edm:matchingPattern>
  </edm:PersistentIdentifierScheme>
</rdf:RDF>